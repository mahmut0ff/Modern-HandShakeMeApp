{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{ts,tsx}' --coveragePathIgnorePatterns='/node_modules/' --coverageReporters='text' --coverageReporters='lcov' --coverageReporters='html'",
    "test:unit": "jest --testMatch='**/__tests__/unit/**/*.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.tsx'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios:debug": "detox test --configuration ios.sim.debug --loglevel trace",
    "test:e2e:android:debug": "detox test --configuration android.emu.debug --loglevel trace",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "test:all": "npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@reduxjs/toolkit": "^2.5.0",
    "async-mutex": "^0.4.0",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-device": "~7.0.1",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-image-manipulator": "~14.0.0",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "~16.0.0",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~14.0.0",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "i18n-js": "^4.4.3",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.4.1",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.3.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.9",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
