{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "__tests__",
    "e2e",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.property.test.tsx"
  ]
}